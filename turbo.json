{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": ["**/.env.*local"],
    "pipeline": {
        "build:rollup": {
            "outputs": ["dist/**"],
            "dependsOn": ["^build:rollup"]
        },
        "build:tsup": {
            "outputs": ["dist/**"],
            "dependsOn": ["^build:tsup"]
        },
        "build:rollup:prod": {
            "outputs": ["dist/**"],
            "dependsOn": ["^build:rollup:prod"]
        },
        "test:e2e": {
            "outputs": [],
            "env": ["TURBO_CACHE_ENABLED"]
        },
        "checks": {
            "dependsOn": []
        }
    }
}