{
  "name": "@flatfile/utils-testing",
  "version": "0.3.1",
  "description": "A utility package for testing plugins",
  "engines": {
    "node": ">= 16"
  },
  "type": "module",
  "exports": {
    ".": {
      "node": {
        "types": {
          "import": "./dist/index.d.ts",
          "require": "./dist/index.d.cts"
        },
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "browser": {
        "types": {
          "import": "./dist/index.d.ts",
          "require": "./dist/index.d.cts"
        },
        "import": "./dist/index.browser.js",
        "require": "./dist/index.browser.cjs"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "build:prod": "NODE_ENV=production tsup",
    "check-exports": "attw --pack .",
    "lint": "tsc",
    "test": "vitest run src/*.spec.ts",
    "test:unit": "vitest run src/*.spec.ts --exclude '**/*.e2e.spec.ts'",
    "test:e2e": "vitest --mode defaults run src/*.e2e.spec.ts --passWithNoTests"
  },
  "author": "Carl Brugger",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlatFilers/flatfile-plugins.git",
    "directory": "utils/testing"
  },
  "license": "ISC",
  "dependencies": {
    "@flatfile/api": "^1.9.19",
    "@flatfile/listener": "^1.1.0",
    "@flatfile/listener-driver-pubsub": "^2.0.0",
    "cross-fetch": "^4.0.0"
  },
  "devDependencies": {
    "@flatfile/bundler-config-tsup": "^0.0.0"
  }
}
