{
  "name": "@flatfile/plugin-ai-generator",
  "version": "1.0.0",
  "description": "A Flatfile plugin that generates example records using AI",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "source": "./src/index.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "build:prod": "NODE_ENV=production rollup -c",
    "check": "tsc ./**/*.ts --noEmit --esModuleInterop",
    "test": "jest ./**/*.spec.ts --config=../../jest.config.js --runInBand"
  },
  "keywords": [
    "flatfile",
    "plugin",
    "ai",
    "generator",
    "flatfile-plugins",
    "category-transform"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@flatfile/listener": "^1.0.5",
    "@flatfile/util-common": "^1.4.0",
    "@flatfile/api": "^1.9.15",
    "@anthropic-ai/sdk": "^0.27.3"
  },
  "devDependencies": {
    "@flatfile/hooks": "^1.5.0",
    "@flatfile/rollup-config": "^0.1.1",
    "typescript": "^5.6.2",
    "rollup": "^4.22.4",
    "@types/node": "^22.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YourGitHubUsername/flatfile-plugin-ai-generator.git"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ]
}
