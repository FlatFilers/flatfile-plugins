    "axios": "^1.7.7",
    "fast-xml-parser": "^4.5.0"
  },
  "peerDependencies": {
    "@flatfile/listener": "^1.0.5"
  },
  "devDependencies": {
    "@flatfile/hooks": "^1.5.0",
    "@flatfile/rollup-config": "^0.1.1",
    "typescript": "^5.6.2",
    "@types/node": "^22.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FlatFilers/flatfile-plugins.git",
    "directory": "plugins/strava-gpx-fetcher"
