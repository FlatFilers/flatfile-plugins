{
  "name": "@flatfile/common-plugin-utils",
  "version": "1.0.2",
  "url": "https://github.com/FlatFilers/flatfile-plugins/tree/main/support/common-utils",
  "description": "A library containing common utilities and helpers for plugins",
  "keywords": [],
  "author": "Flatfile",
  "type": "module",
  "engines": {
    "node": ">= 16"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "exports": {
    ".": {
      "node": {
        "types": {
          "import": "./dist/index.d.ts",
          "require": "./dist/index.d.cts"
        },
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "browser": {
        "types": {
          "import": "./dist/index.d.ts",
          "require": "./dist/index.d.cts"
        },
        "import": "./dist/index.browser.js",
        "require": "./dist/index.browser.cjs"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "build:prod": "NODE_ENV=production tsup",
    "checks": "tsc --noEmit && attw --pack . && publint . && publint deps",
    "lint": "tsc --noEmit",
    "test": "jest src/*.spec.ts --detectOpenHandles",
    "test:unit": "jest src/*.spec.ts --testPathIgnorePatterns=.*\\.e2e\\.spec\\.ts$ --detectOpenHandles",
    "test:e2e": "jest src/*.e2e.spec.ts --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlatFilers/flatfile-plugins.git",
    "directory": "support/common-utils"
  },
  "license": "ISC",
  "devDependencies": {
    "@flatfile/bundler-config-tsup": "^0.0.0"
  }
}
