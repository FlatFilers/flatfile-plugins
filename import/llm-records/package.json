{
  "name": "@flatfile/plugin-import-llm-records",
  "version": "0.1.0",
  "url": "https://github.com/FlatFilers/flatfile-plugins/tree/main/import/llm-records",
  "description": "A Flatfile plugin that generates example records using AI",
  "registryMetadata": {
    "category": "import"
  },
  "engines": {
    "node": ">= 16"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "browser": {
    "./dist/index.cjs": "./dist/index.browser.cjs",
    "./dist/index.mjs": "./dist/index.browser.mjs"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "node": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "browser": {
      "require": "./dist/index.browser.cjs",
      "import": "./dist/index.browser.mjs"
    },
    "default": "./dist/index.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "build:prod": "NODE_ENV=production rollup -c",
    "check": "tsc ./**/*.ts --noEmit --esModuleInterop",
    "test": "jest src/*.spec.ts --detectOpenHandles",
    "test:unit": "jest src/*.spec.ts --testPathIgnorePatterns=.*\\.e2e\\.spec\\.ts$ --detectOpenHandles",
    "test:e2e": "jest src/*.e2e.spec.ts --detectOpenHandles"
  },
  "keywords": [
    "flatfile-plugins",
    "category-import"
  ],
  "author": "Flatfile, Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlatFilers/flatfile-plugins.git",
    "directory": "import/llm-records"
  },
  "license": "ISC",
  "dependencies": {
    "@flatfile/plugin-job-handler": "^0.6.1",
    "@flatfile/util-common": "^1.4.0"
  },
  "peerDependencies": {
    "@flatfile/api": "^1.9.19",
    "@flatfile/listener": "^1.1.0",
    "@langchain/anthropic": "^0.3.5",
    "@langchain/core": "^0.3.13",
    "@langchain/mistralai": "^0.1.1",
    "@langchain/openai": "^0.3.8"
  },
  "devDependencies": {
    "@flatfile/bundler-config-rollup": "^0.1.1"
  }
}
