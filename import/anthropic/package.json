{
  "name": "@flatfile/plugin-import-anthropic",
  "version": "0.0.0",
  "url": "https://github.com/FlatFilers/flatfile-plugins/tree/main/import/anthropic",
  "description": "A Flatfile plugin that generates example records using Anthropic",
  "registryMetadata": {
    "category": "import"
  },
  "engines": {
    "node": ">= 16"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "browser": {
    "./dist/index.cjs": "./dist/index.browser.cjs",
    "./dist/index.mjs": "./dist/index.browser.mjs"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "node": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "browser": {
      "require": "./dist/index.browser.cjs",
      "import": "./dist/index.browser.mjs"
    },
    "default": "./dist/index.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "build:prod": "NODE_ENV=production rollup -c",
    "check": "tsc ./**/*.ts --noEmit --esModuleInterop",
    "test": "jest src/*.spec.ts --detectOpenHandles",
    "test:unit": "jest src/*.spec.ts --testPathIgnorePatterns=.*\\.e2e\\.spec\\.ts$ --detectOpenHandles",
    "test:e2e": "jest src/*.e2e.spec.ts --detectOpenHandles"
  },
  "keywords": [
    "flatfile-plugins",
    "category-import"
  ],
  "author": "Flatfile, Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlatFilers/flatfile-plugins.git",
    "directory": "import/anthropic"
  },
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3"
  },
  "peerDependencies": {
    "@flatfile/api": "^1.9.19",
    "@flatfile/util-common": "^1.4.0",
    "@flatfile/plugin-job-handler": "^0.6.1"
  },
  "devDependencies": {
    "@flatfile/rollup-config": "^0.1.1"
  }
}
