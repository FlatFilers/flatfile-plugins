{
  "name": "@flatfile/plugin-graphql-schema",
  "version": "1.2.2",
  "description": "A plugin for converting GraphQL to Flatfile blueprint.",
  "registryMetadata": {
    "category": "core"
  },
  "browser": {
    "./dist/index.cjs": "./dist/index.browser.cjs",
    "./dist/index.mjs": "./dist/index.browser.mjs"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "default": "./dist/index.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "build:prod": "NODE_ENV=production rollup -c",
    "check": "tsc ./**/*.ts --noEmit --esModuleInterop",
    "test": "bun --env-file=../../.env.defaults test"
  },
  "keywords": [],
  "author": "Flatfile, Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlatFilers/flatfile-plugins.git",
    "directory": "plugins/graphql-schema"
  },
  "type": "module",
  "license": "ISC",
  "dependencies": {
    "@flatfile/plugin-space-configure": "^0.5.1",
    "change-case": "^5.4.3",
    "cross-fetch": "^4.0.0",
    "graphql": "^16.8.1"
  },
  "peerDependencies": {
    "@flatfile/listener": "^1.0.1"
  },
  "devDependencies": {
    "@flatfile/rollup-config": "0.1.1"
  }
}
