{
  "name": "@flatfile/plugin-dedupe",
  "version": "1.1.0",
  "description": "Dedupe records in a sheet via a sheet level custom action.",
  "registryMetadata": {
    "category": "transform"
  },
  "engines": {
    "node": ">= 16"
  },
  "browser": {
    "./dist/index.cjs": "./dist/index.browser.cjs",
    "./dist/index.mjs": "./dist/index.browser.mjs"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "node": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "browser": {
      "require": "./dist/index.browser.cjs",
      "import": "./dist/index.browser.mjs"
    },
    "default": "./dist/index.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "build:prod": "NODE_ENV=production rollup -c",
    "check": "tsc ./**/*.ts --noEmit --esModuleInterop",
    "test": "jest ./**/*.spec.ts --config=../../jest.config.js --runInBand"
  },
  "keywords": [
    "flatfile-plugins",
    "category-transform"
  ],
  "author": "Flatfile, Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlatFilers/flatfile-plugins.git",
    "directory": "plugins/dedupe"
  },
  "license": "ISC",
  "dependencies": {
    "@flatfile/plugin-job-handler": "^0.5.0",
    "@flatfile/util-common": "^1.3.0"
  },
  "peerDependencies": {
    "@flatfile/api": "^1.7.4",
    "@flatfile/listener": "^1.0.1"
  },
  "devDependencies": {
    "@flatfile/api": "^1.7.4",
    "@faker-js/faker": "^7.6.0",
    "@flatfile/rollup-config": "0.1.0"
  }
}
