{
  "name": "@flatfile/plugin-dxp-configure",
  "version": "0.2.0",
  "description": "A plugin for using DXP class-based configurations.",
  "registryMetadata": {
    "category": "utilities"
  },
  "engines": {
    "node": ">= 12"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "browser": {
    "./dist/index.cjs": "./dist/index.browser.cjs",
    "./dist/index.mjs": "./dist/index.browser.mjs"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "default": "./dist/index.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "check": "tsc ./**/*.ts --noEmit --esModuleInterop",
    "test": "jest ./**/*.spec.ts --config=../../jest.config.js --runInBand"
  },
  "keywords": [],
  "author": "David Boskovic",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlatFilers/flatfile-plugins.git",
    "directory": "plugins/dxp-configure"
  },
  "license": "ISC",
  "dependencies": {
    "@flatfile/api": "^1.6.3",
    "@flatfile/listener": "^1.0.0"
  },
  "devDependencies": {
    "@flatfile/configure": "^0.5.39",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "rollup": "^4.3.0"
  }
}
