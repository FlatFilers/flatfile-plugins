{
  "name": "@flatfile/plugin-psv-extractor",
  "version": "1.10.0",
  "url": "https://github.com/FlatFilers/flatfile-plugins/tree/main/plugins/psv-extractor",
  "description": "A plugin for parsing a psv file and extracting it into Flatfile.",
  "registryMetadata": {
    "category": "extractors"
  },
  "type": "module",
  "engines": {
    "node": ">= 16"
  },
  "exports": {
    ".": {
      "node": {
        "types": {
          "import": "./dist/index.d.ts",
          "require": "./dist/index.d.cts"
        },
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "browser": {
        "types": {
          "import": "./dist/index.d.ts",
          "require": "./dist/index.d.cts"
        },
        "import": "./dist/index.browser.js",
        "require": "./dist/index.browser.cjs"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "build:prod": "NODE_ENV=production tsup",
    "checks": "tsc --noEmit && attw --pack . && publint .",
    "lint": "tsc --noEmit",
    "test": "vitest run --mode defaults src/*.spec.ts --passWithNoTests",
    "test:unit": "vitest run --mode defaults src/*.spec.ts --passWithNoTests --exclude src/*.e2e.spec.ts",
    "test:e2e": "vitest run --mode defaults src/*.e2e.spec.ts --passWithNoTests --no-file-parallelism"
  },
  "keywords": [],
  "author": "Ashley Mulligan",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlatFilers/flatfile-plugins.git",
    "directory": "plugins/psv-parser"
  },
  "license": "ISC",
  "dependencies": {},
  "peerDependencies": {
    "@flatfile/api": "^1.9.19",
    "@flatfile/listener": "^1.1.0"
  },
  "devDependencies": {
    "@flatfile/bundler-config-tsup": "^0.1.0",
    "@flatfile/config-vitest": "^0.0.0"
  }
}
