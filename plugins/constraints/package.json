{
  "name": "@flatfile/plugin-constraints",
  "version": "1.1.8",
  "description": "A plugin for extending blueprint with external constraints",
  "engines": {
    "node": ">= 16"
  },
  "registryMetadata": {
    "category": "records"
  },
  "browser": {
    "./dist/index.cjs": "./dist/index.browser.cjs",
    "./dist/index.mjs": "./dist/index.browser.mjs"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "node": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "browser": {
      "require": "./dist/index.browser.cjs",
      "import": "./dist/index.browser.mjs"
    },
    "default": "./dist/index.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "build:prod": "NODE_ENV=production rollup -c",
    "check": "tsc ./**/*.ts --noEmit --esModuleInterop",
    "test": "jest ./**/*.spec.ts --config=../../jest.config.js --runInBand"
  },
  "keywords": [],
  "author": "David Boskovic",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlatFilers/flatfile-plugins.git",
    "directory": "plugins/constraints"
  },
  "license": "ISC",
  "dependencies": {
    "@flatfile/plugin-record-hook": "^1.4.7"
  },
  "peerDependencies": {
    "@flatfile/api": "^1.7.4",
    "@flatfile/listener": "^1.0.1"
  },
  "devDependencies": {
    "@flatfile/api": "^1.7.4",
    "@flatfile/rollup-config": "0.1.0"
  }
}
