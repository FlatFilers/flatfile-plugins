{
  "name": "@flatfile/plugin-constraints",
  "version": "3.0.1",
  "description": "A plugin for extending blueprint with external constraints",
  "type": "module",
  "engines": {
    "node": ">= 16"
  },
  "registryMetadata": {
    "category": "records"
  },
  "exports": {
    ".": {
      "node": {
        "types": {
          "import": "./dist/index.d.ts",
          "require": "./dist/index.d.cts"
        },
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "browser": {
        "types": {
          "import": "./dist/index.d.ts",
          "require": "./dist/index.d.cts"
        },
        "import": "./dist/index.browser.js",
        "require": "./dist/index.browser.cjs"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "build:prod": "NODE_ENV=production tsup",
    "checks": "tsc && attw --pack . && publint . && publint deps",
    "lint": "tsc",
    "test": "jest src/*.spec.ts --detectOpenHandles",
    "test:unit": "jest src/*.spec.ts --testPathIgnorePatterns=.*\\.e2e\\.spec\\.ts$ --detectOpenHandles",
    "test:e2e": "jest src/*.e2e.spec.ts --detectOpenHandles"
  },
  "keywords": [],
  "author": "David Boskovic",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlatFilers/flatfile-plugins.git",
    "directory": "plugins/constraints"
  },
  "license": "ISC",
  "dependencies": {},
  "peerDependencies": {
    "@flatfile/api": "^1.9.19",
    "@flatfile/listener": "^1.1.0",
    "@flatfile/plugin-record-hook": "^1.7.1"
  },
  "devDependencies": {
    "@flatfile/plugin-record-hook": "^1.7.1",
    "@flatfile/bundler-config-tsup": "^0.0.0"
  }
}
