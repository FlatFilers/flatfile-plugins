{
  "name": "@flatfile/plugin-record-hook",
  "version": "1.2.0",
  "description": "A plugin for writing custom record-level hooks in Flatfile.",
  "registryMetadata": {
    "category": "records"
  },
  "engines": {
    "node": ">= 12"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "browser": {
    "./dist/index.cjs": "./dist/index.browser.cjs",
    "./dist/index.mjs": "./dist/index.browser.mjs"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "check": "tsc ./**/*.ts --noEmit --esModuleInterop",
    "test": "jest ./src/tests/*.spec.ts --config=../../jest.config.js --runInBand"
  },
  "keywords": [],
  "author": "Alex Hollenbeck",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlatFilers/flatfile-plugins.git",
    "directory": "plugins/record-hook"
  },
  "license": "ISC",
  "dependencies": {
    "@flatfile/api": "^1.6.0",
    "@flatfile/hooks": "^1.3.1",
    "@flatfile/listener": "^0.3.17",
    "@flatfile/util-common": "^0.2.3",
    "effect": "^2.0.0-next.29"
  },
  "devDependencies": {
    "@flatfile/utils-testing": "^0.0.6",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "axios": "^1.4.0",
    "rollup": "^4.3.0"
  }
}
