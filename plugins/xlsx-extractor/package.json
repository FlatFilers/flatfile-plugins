{
  "name": "@flatfile/plugin-xlsx-extractor",
  "version": "3.2.2",
  "url": "https://github.com/FlatFilers/flatfile-plugins/tree/main/plugins/xlsx-extractor",
  "description": "A plugin for parsing xlsx files in Flatfile.",
  "registryMetadata": {
    "category": "extractors"
  },
  "type": "module",
  "engines": {
    "node": ">= 16"
  },
  "exports": {
    ".": {
      "node": {
        "types": {
          "import": "./dist/index.d.ts",
          "require": "./dist/index.d.cts"
        },
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "build:prod": "NODE_ENV=production tsup",
    "checks": "tsc && attw --pack . && publint . && publint deps",
    "test": "jest src/*.spec.ts --detectOpenHandles",
    "test:unit": "jest src/*.spec.ts --testPathIgnorePatterns=.*\\.e2e\\.spec\\.ts$ --detectOpenHandles",
    "test:e2e": "jest src/*.e2e.spec.ts --detectOpenHandles"
  },
  "keywords": [
    "flatfile-plugins",
    "category-extractors"
  ],
  "author": "David Boskovic",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlatFilers/flatfile-plugins.git",
    "directory": "plugins/xlsx-extractor"
  },
  "license": "ISC",
  "dependencies": {
    "@flatfile/util-extractor": "^2.1.7",
    "remeda": "^1.14.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz"
  },
  "peerDependencies": {
    "@flatfile/listener": "^1.1.0"
  },
  "devDependencies": {
    "@flatfile/bundler-config-tsup": "^0.0.0"
  }
}
