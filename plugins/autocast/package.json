{
  "name": "@flatfile/plugin-autocast",
  "version": "0.3.4",
  "description": "A plugin for automatically casting values in Flatfile.",
  "registryMetadata": {
    "category": "transform"
  },
  "engines": {
    "node": ">= 16"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "browser": {
    "./dist/index.cjs": "./dist/index.browser.cjs",
    "./dist/index.mjs": "./dist/index.browser.mjs"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "check": "tsc ./**/*.ts --noEmit --esModuleInterop",
    "test": "TZ=GMT jest ./**/*.spec.ts --config=../../jest.config.js --runInBand"
  },
  "keywords": [],
  "author": "Alex Hollenbeck",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlatFilers/flatfile-plugins.git",
    "directory": "plugins/autocast"
  },
  "license": "ISC",
  "dependencies": {
    "@flatfile/api": "^1.5.45",
    "@flatfile/listener": "^0.3.17",
    "@flatfile/plugin-record-hook": "^1.1.12",
    "@flatfile/util-common": "^0.2.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "rollup": "^4.3.0"
  }
}
