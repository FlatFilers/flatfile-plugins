{
  "name": "@flatfile/plugin-autocast",
  "version": "2.0.1",
  "url": "https://github.com/FlatFilers/flatfile-plugins/tree/main/plugins/autocast",
  "description": "A plugin for automatically casting values in Flatfile.",
  "registryMetadata": {
    "category": "transform"
  },
  "engines": {
    "node": ">= 16"
  },
  "browser": {
    "./dist/index.cjs": "./dist/index.browser.cjs",
    "./dist/index.mjs": "./dist/index.browser.mjs"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "node": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "browser": {
      "require": "./dist/index.browser.cjs",
      "import": "./dist/index.browser.mjs"
    },
    "default": "./dist/index.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "build:prod": "NODE_ENV=production rollup -c",
    "check": "tsc ./**/*.ts --noEmit --esModuleInterop",
    "test": "jest src/*.spec.ts --config=../../jest.config.js"
  },
  "keywords": [
    "flatfile-plugins",
    "category-transform",
    "featured"
  ],
  "author": "Alex Hollenbeck",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlatFilers/flatfile-plugins.git",
    "directory": "plugins/autocast"
  },
  "license": "ISC",
  "dependencies": {
    "@flatfile/hooks": "^1.4.1",
    "@flatfile/util-common": "^1.4.1"
  },
  "peerDependencies": {
    "@flatfile/api": "^1.9.19",
    "@flatfile/listener": "^1.1.0",
    "@flatfile/plugin-record-hook": "^1.7.1"
  },
  "devDependencies": {
    "@flatfile/plugin-record-hook": "^1.7.1",
    "@flatfile/rollup-config": "0.1.1"
  }
}
