{
  "name": "@flatfile/plugin-extract-html-table",
  "version": "1.1.0",
  "url": "https://github.com/FlatFilers/flatfile-plugins/tree/main/extract/html-table",
  "description": "A Flatfile plugin for extracting table data from HTML files",
  "registryMetadata": {
    "category": "extract"
  },
  "type": "module",
  "engines": {
    "node": ">= 16"
  },
  "exports": {
    ".": {
      "node": {
        "types": {
          "import": "./dist/index.d.ts",
          "require": "./dist/index.d.cts"
        },
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "browser": {
        "types": {
          "import": "./dist/index.d.ts",
          "require": "./dist/index.d.cts"
        },
        "import": "./dist/index.browser.js",
        "require": "./dist/index.browser.cjs"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "build:prod": "NODE_ENV=production tsup",
    "check-exports": "attw --pack .",
    "lint": "tsc",
    "test": "vitest --passWithNoTests",
    "test:unit": "vitest run src/*.spec.ts --exclude '**/*.e2e.spec.ts' --passWithNoTests",
    "test:e2e": "vitest --mode defaults run src/*.e2e.spec.ts --passWithNoTests"
  },
  "keywords": [
    "flatfile",
    "flatfile-plugins",
    "category-extractors",
    "html",
    "table-extractor"
  ],
  "author": "Flatfile",
  "license": "ISC",
  "dependencies": {
    "@flatfile/util-extractor": "^2.1.5",
    "node-html-parser": "^6.1.13"
  },
  "devDependencies": {
    "@flatfile/bundler-config-tsup": "^0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FlatFilers/flatfile-plugins.git",
    "directory": "extract/html-table"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ]
}