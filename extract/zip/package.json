{
  "name": "@flatfile/plugin-zip-extractor",
  "version": "0.6.1",
  "url": "https://github.com/FlatFilers/flatfile-plugins/tree/main/extract/zip",
  "description": "A plugin for unzipping zip files and uploading content back in Flatfile.",
  "registryMetadata": {
    "category": "extract"
  },
  "engines": {
    "node": ">= 16"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "browser": {
    "./dist/index.cjs": "./dist/index.browser.cjs",
    "./dist/index.mjs": "./dist/index.browser.mjs"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "node": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "browser": {
      "require": "./dist/index.browser.cjs",
      "import": "./dist/index.browser.mjs"
    },
    "default": "./dist/index.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "build:prod": "NODE_ENV=production rollup -c",
    "check": "tsc ./**/*.ts --noEmit --esModuleInterop",
    "test": "jest src/*.spec.ts --detectOpenHandles",
    "test:unit": "jest src/*.spec.ts --testPathIgnorePatterns=.*\\.e2e\\.spec\\.ts$ --detectOpenHandles",
    "test:e2e": "jest src/*.e2e.spec.ts --detectOpenHandles"
  },
  "keywords": [
    "flatfile-plugins",
    "category-extract"
  ],
  "author": "Carl Brugger",
  "repository": {
    "type": "git",
    "url": "https://github.com/FlatFilers/flatfile-plugins.git",
    "directory": "extract/zip"
  },
  "license": "ISC",
  "dependencies": {
    "@flatfile/plugin-job-handler": "^0.6.1",
    "@flatfile/util-common": "^1.4.1",
    "@flatfile/util-file-buffer": "^0.4.1",
    "adm-zip": "^0.5.10",
    "modern-async": "^2.0.0"
  },
  "peerDependencies": {
    "@flatfile/api": "^1.9.19",
    "@flatfile/listener": "^1.1.0"
  },
  "devDependencies": {
    "@flatfile/rollup-config": "^0.1.1",
    "@flatfile/utils-testing": "^0.3.1",
    "@types/adm-zip": "^0.4.3"
  }
}
